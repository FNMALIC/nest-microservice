{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/api-gateway/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/api-gateway/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/api-gateway",
  "projects": {
    "api-gateway": {
      "type": "application",
      "root": "apps/api-gateway",
      "entryFile": "main",
      "sourceRoot": "apps/api-gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api-gateway/tsconfig.app.json"
      }
    },
    "academic-years": {
      "type": "application",
      "root": "apps/academic-years",
      "entryFile": "main",
      "sourceRoot": "apps/academic-years/src",
      "compilerOptions": {
        "tsConfigPath": "apps/academic-years/tsconfig.app.json"
      }
    },
    "lessons": {
      "type": "application",
      "root": "apps/lessons",
      "entryFile": "main",
      "sourceRoot": "apps/lessons/src",
      "compilerOptions": {
        "tsConfigPath": "apps/lessons/tsconfig.app.json"
      }
    },
    "users": {
      "type": "application",
      "root": "apps/users",
      "entryFile": "main",
      "sourceRoot": "apps/users/src",
      "compilerOptions": {
        "tsConfigPath": "apps/users/tsconfig.app.json"
      }
    },
    "classes": {
      "type": "application",
      "root": "apps/classes",
      "entryFile": "main",
      "sourceRoot": "apps/classes/src",
      "compilerOptions": {
        "tsConfigPath": "apps/classes/tsconfig.app.json"
      }
    },
    "schedules": {
      "type": "application",
      "root": "apps/schedules",
      "entryFile": "main",
      "sourceRoot": "apps/schedules/src",
      "compilerOptions": {
        "tsConfigPath": "apps/schedules/tsconfig.app.json"
      }
    },
    "request-batch": {
      "type": "application",
      "root": "apps/request-batch",
      "entryFile": "main",
      "sourceRoot": "apps/request-batch/src",
      "compilerOptions": {
        "tsConfigPath": "apps/request-batch/tsconfig.app.json"
      }
    },
    "request-batchs": {
      "type": "application",
      "root": "apps/request-batchs",
      "entryFile": "main",
      "sourceRoot": "apps/request-batchs/src",
      "compilerOptions": {
        "tsConfigPath": "apps/request-batchs/tsconfig.app.json"
      }
    },
    "request-steps": {
      "type": "application",
      "root": "apps/request-steps",
      "entryFile": "main",
      "sourceRoot": "apps/request-steps/src",
      "compilerOptions": {
        "tsConfigPath": "apps/request-steps/tsconfig.app.json"
      }
    }
  }
}