{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/microservices-demo/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
